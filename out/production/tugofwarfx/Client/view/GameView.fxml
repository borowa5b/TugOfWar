<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane onKeyPressed="#keyAction" prefHeight="200.0" prefWidth="300.0" stylesheets="@styles/styles.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.view.GameViewController">
    <BorderPane prefHeight="200.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <center>
            <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.5" styleClass="progress" BorderPane.alignment="CENTER" />
        </center>
      <bottom>
         <Label fx:id="endGameText" BorderPane.alignment="CENTER" />
      </bottom>
    </BorderPane>
</AnchorPane>
